<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:padding="10dip"
	android:layout_width="fill_parent" android:layout_height="wrap_content">
	<LinearLayout android:id="@+id/LinearLayoutComputeUP" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="100sp">
	</LinearLayout><LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/LinearLayout01Spinner1">
		<TextView android:text="Store Name" android:layout_width="50sp"
			android:layout_height="50sp" android:textSize="15sp"
			android:id="@+id/TextViewStoreName_computeR"></TextView>
		<Spinner android:id="@+id/Spinner01" android:layout_width="190sp"
			android:layout_height="50sp"></Spinner>
		<Button android:text="Edit" android:layout_height="50sp"
			android:onClick="@string/EditStoreName" android:layout_width="50sp"
			android:id="@+id/ButtonEditStorname"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/LinearLayout02Spinner2">
		<TextView android:text="Date:" android:layout_width="50sp"
			android:layout_height="50sp" android:textSize="15sp"
			android:id="@+id/TextView02Date_computeR"></TextView>
		<Spinner android:id="@+id/Spinner02" android:layout_width="190sp"
			android:layout_height="50sp"></Spinner>
		<Button android:text="Edit" android:layout_height="50sp"
			android:layout_width="50sp" android:id="@+id/Button02EditDate"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/LinearLayout03Spinner3">
		<TextView android:text="Price:" android:layout_width="50sp"
			android:layout_height="50sp" android:textSize="15sp"
			android:id="@+id/TextView03PriceComputeR"></TextView>
		<Spinner android:id="@+id/Spinner03" android:layout_width="190sp"
			android:layout_height="50sp"></Spinner>
		<Button android:text="Edit" android:layout_height="50sp"
			android:onClick="@string/getDialogPrice" android:layout_width="50sp"
			android:id="@+id/Button03EditPrice"></Button>
	</LinearLayout>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/LinearLayout04Spinner4">
		<TextView android:text="Cat:" android:layout_width="50sp"
			android:layout_height="50sp" android:textSize="15sp"
			android:id="@+id/TextView03CatComputeR"></TextView>
		<Spinner android:id="@+id/Spinner04" android:layout_width="190sp"
			android:layout_height="50sp" android:addStatesFromChildren="true"
			android:clipChildren="true" android:clipToPadding="true"></Spinner>
		<Button android:text="Edit" android:layout_height="50sp"
			android:onClick="@string/CatEdit" android:layout_width="50sp"
			android:id="@+id/Button04EditCat"></Button>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/Button01saveandreturn" android:onClick="onClick" android:text="save and return"></Button>
	</LinearLayout>




</LinearLayout>
